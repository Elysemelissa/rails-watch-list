<br>
<h3><%= @list.name %></h3>

<p>
  Created on: <%= @list.created_at.strftime("%B %d, %Y") %>
</p>

<% @list.bookmarks.each do |bookmark| %>
  <p><%= bookmark.comment %></p>
  <p><%= bookmark.movie.title %></p>
  <%= image_tag(bookmark.movie.poster_url)  %>
<% end %>

<%= link_to "Back to Lists", "/lists" %>